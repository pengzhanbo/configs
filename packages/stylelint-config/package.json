{
  "name": "@pengzhanbo/stylelint-config",
  "type": "module",
  "version": "1.26.0",
  "author": "pengzhanbo <q942450674@outlook.com> (https://github/pengzhanbo/)",
  "license": "MIT",
  "homepage": "https://github.com/pengzhanbo/configs#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:pengzhanbo/configs.git",
    "directory": "packages/stylelint-config"
  },
  "keywords": [
    "stylelint-config"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --dts --shims",
    "stub": "tsup src/index.ts --format esm",
    "prepublishOnly": "pnpm run build",
    "lint": "stylelint **/*.{css,scss} -c ./stylelint.config.js"
  },
  "peerDependencies": {
    "stylelint": ">=16.0.0"
  },
  "dependencies": {
    "@pengzhanbo/utils": "^1.2.0",
    "@stylelint-types/stylelint-order": "^6.0.4",
    "@stylelint-types/stylelint-scss": "^6.5.0",
    "@stylelint-types/stylelint-stylistic": "^3.1.1",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "local-pkg": "^1.0.0",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-define-config": "^16.14.1",
    "stylelint-order": "^6.0.4"
  },
  "devDependencies": {
    "stylelint": "^16.14.1"
  }
}
